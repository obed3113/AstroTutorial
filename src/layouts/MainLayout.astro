                <img class="w-full mb-4 rounded-lg lg:mb-0 lg:flex" src="../../public/img/OIG2.jpg" alt="here should be a picture">
            </div>                
        </div>
    </section>
    <!-- End block -->

    <!-- Start block -->
    <section class="bg-gray-50 dark:bg-gray-800">
        <div class="max-w-screen-xl px-4 py-2 mx-auto space-y-12 lg:space-y-20 lg:py-4 lg:px-4">
